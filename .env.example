# MVP90 Terminal - API Configuration
# Copy this file to .env.local and fill in your actual API keys

# GitHub API (for developer metrics)
GITHUB_TOKEN=your_github_personal_access_token_here

# Product Hunt API (for startup traction data)
PRODUCTHUNT_TOKEN=your_producthunt_api_token_here

# Supabase (for user data, scoring, and analytics)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Firebase/Clerk Auth (for user authentication)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# OpenRouter (for AI-powered insights - optional)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:8000
NODE_ENV=development

# Rate Limiting
GITHUB_API_RATE_LIMIT=5000
PRODUCTHUNT_API_RATE_LIMIT=1000

# Cache Settings
REDIS_URL=your_redis_url_for_caching_optional
CACHE_TTL_SECONDS=300
